import math

def func1(x):
    if 0 <= x <= 1:
        return x
    else:
        return math.exp(-2)

def func2(x):
    if x >= 0:
        return x
    else:
        return -x

def count_down(x, y):
    print(x)
    if x == y:
        return 0
    else:
        return count_down(x - 1, y)

count_down(5, 0)  
